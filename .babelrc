{
  "comments": false,
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "esmodules": true
        },
        "modules": false,
        "useBuiltIns": false
      }
    ],
    "@babel/typescript",
    "vca-jsx",
    "@vue/app"
  ],
  "plugins": [
    [
      "snowpack/assets/babel-plugin.js", 
      { 
        "optionalExtensions": true 
      }
    ],
    "@babel/plugin-proposal-nullish-coalescing-operator",
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    "@babel/plugin-syntax-dynamic-import",
    "babel-plugin-add-import-extension",
    "transform-vue-jsx"
  ],
  "env": {
    "production": {
      "presets": [
        [
          "minify",
          {
            "mangle": {
              "topLevel": true,
              "keepFnName": false,
              "keepClassName": false
            },
            "keepFnName": false,
            "keepClassName": false,
            "removeConsole": true
          }
        ]
      ]
    }
  }
}
